<!DOCTYPE html>
<html lang="en">

  <%- include('../partials/head.ejs') %>

<head>
  <!-- Required meta tags -->
<meta charset="utf-8">
<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">



<!-- Favicon icon-->
<link rel="shortcut icon" type="image/x-icon" href="../v1Build/assetsimages/favicon/favicon.ico">

<!-- Libs CSS -->

<link rel="stylesheet" href="../v1Build/assets/libs/prismjs/themes/prism.css">
<link rel="stylesheet" href="../v1Build/assets/libs/prismjs/plugins/line-numbers/prism-line-numbers.css">
<link rel="stylesheet" href="../v1Build/assets/libs/prismjs/plugins/toolbar/prism-toolbar.css">
<link rel="stylesheet" href="../v1Build/assets/libs/bootstrap-icons/font/bootstrap-icons.css">
<link rel="stylesheet" href="../v1Build/assets/libs/dropzone/dist/dropzone.css" >
<link href="../v1Build/assets/libs/@mdi/font/css/materialdesignicons.min.css" rel="stylesheet" />

<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">






<!-- Theme CSS -->
<link rel="stylesheet" href="../v1Build/assets/css/theme.min.css">
  <title>ADMIN View topic Page</title>
</head>

<body>
  <!-- container -->
  
        <!-- Card -->
        <div class="card-header text-center">
          <a href="" class="h1"><b>Coachers || Admin</b></a>
        </div>

        <div class="card smooth-shadow-md">

          <!-- Card body -->
          <div class="card-body p-6">
            <div class="mb-4">
              <!--<a href="../index.html"><img src="../v1Build/assetsimages/brand/logo/logo-primary.svg" class="mb-2" alt=""></a>-->
              <p class="mb-6"> Topics Currently Registered</p>
            </div>
            <!-- Form -->

            <!-- <List></List> -->
            <div class="x_panel">
            <div class="x_title">
              <div class="clearfix"></div>
            </div>
            <div class="x_content">
                <br/>
                
                <div class="row">
                    <div class="col-sm-12">
                        <div class="card-box table-responsive">

                            <table id="datatable-responsive"
                                class="table table-striped table-bordered dt-responsive nowrap" cellspacing="0"
                                width="100%">
                                <thead>
                                    <tr>
                                        <th>Subject Name</th>
                                        <th>Topic Name</th>
                                      
                                        <th></th>
                                        
                                    </tr>
                                </thead>
                
                                <tbody>
                                  <% if(participants.length>0){ %>
                                    <% participants.forEach(participant=>{ %>
                                        <tr></tr>
                                        <td> <%= participant.subject %> </td>
                                    
                                        <td> <%= participant.name %> </td>

                                        <td>
                                            <% const deleteURL = 'deletetopic/' + participant._id + '&' + participant.subjectName %>
                                            <a href = "<%= deleteURL %>">
                                                <i class="fa fa-trash"></i>
                                                <span class="fa fa-chevron"></span>
                                            </a>
                                        
                                        </td>
                                    <% }) %>
                                <% } %>

                                </tbody>
                            </table>
                        </div>
                    </div>
                    <div>
                      <a href="/admin/dashboard" class="text-inherit
                          fs-5">Home</a>
                    </div>
                </div>
            </div>
        </div>

       
        
     </div>
      
  <!-- Scripts -->
  <!-- Libs JS -->
<script src="../v1Build/assets/libs/jquery/dist/jquery.min.js"></script>
<script src="../v1Build/assets/libs/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
<script src="../v1Build/assets/libs/jquery-slimscroll/jquery.slimscroll.min.js"></script>
<script src="../v1Build/assets/libs/feather-icons/dist/feather.min.js"></script>
<script src="../v1Build/assets/libs/prismjs/components/prism-core.min.js"></script>
<script src="../v1Build/assets/libs/prismjs/components/prism-markup.min.js"></script>
<script src="../v1Build/assets/libs/prismjs/plugins/line-numbers/prism-line-numbers.min.js"></script>
<script src="../v1Build/assets/libs/apexcharts/dist/apexcharts.min.js"></script>
<script src="../v1Build/assets/libs/dropzone/dist/min/dropzone.min.js"></script>










<!-- clipboard -->
<script src="https://cdnjs.cloudflare.com/ajax/libs/clipboard.js/1.5.12/clipboard.min.js"></script>



<!-- Theme JS -->
<script src="../v1Build/assets/js/theme.min.js"></script>
</body>

</html>