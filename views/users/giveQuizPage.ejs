<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">

  <meta http-equiv="X-UA-Compatible" content="ie=edge">

  <meta name="copyright" content="MACode ID, https://macodeid.com/">

  <title>COACHERS - Dashboard</title>

  <link rel="stylesheet" href="/seogram/assets/css/maicons.css">

  <link rel="stylesheet" href="/seogram/assets/css/bootstrap.css">

  <link rel="stylesheet" href="/seogram/assets/vendor/animate/animate.css">

  <link rel="stylesheet" href="/seogram/assets/css/theme.css">

</head>
<body>

  <!-- Back to top button -->
  <div class="back-to-top"></div>

  <!-- Header -->
  <%- include('./userPartials/headerNavbar.ejs') %>
  <!-- header end -->

  <div class="page-section bg-light">
    <div style="padding-left: 10%;">
        <h1>this is quiz page</h1>

        <% questionsList.forEach(question=>{   %>
        
          <div class="quizpanel">
              <div id="question" style="font-weight: bold;"><%= question.question %></div>
              <div><input type="radio" id="option1" name="options"><span id="optt1"><span style="font-weight: bold;">A: </span> <%= question.A %> </span></div>
              <div><input type="radio" id="option2" name="options"><span id="optt2"><span style="font-weight: bold;">B: </span><%= question.B %></span></div>
              <div><input type="radio" id="option3" name="options"><span id="optt3"><span style="font-weight: bold;">C: </span><%= question.C %></span></div>
              <div><input type="radio" id="option4" name="options"><span id="optt4"><span style="font-weight: bold;">D: </span><%= question.D %></span></div>
          </div>
        </br>
      <% }) %>
    </div>
    <div class="container">
     

    </div> <!-- .container -->
  </div> <!-- .page-section -->

 
  

  <%- include('./userPartials/footer.ejs') %>

<script src="/seogram/assets/js/jquery-3.5.1.min.js"></script>

<script src="/seogram/assets/js/bootstrap.bundle.min.js"></script>

<script src="/seogram/assets/js/google-maps.js"></script>

<script src="/seogram/assets/vendor/wow/wow.min.js"></script>

<script src="/seogram/assets/js/theme.js"></script>
  
</body>
</html>